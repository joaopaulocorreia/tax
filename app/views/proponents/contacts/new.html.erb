<%= render 'proponents/layout' do %>
  <div class='row'>
    <div class='col col-6'>
      <div class='card'>
        <div class='card-header'>
          <h6>New</h6>
        </div>
        <div class='card-body'>
          <%= render 'proponents/contacts/form', contact: @contact, proponent_id: @proponent.id  %>
        </div>
      </div>
    </div>
  </div>

  <div class='row mt-3'>
    <div class='col col-12'>
      <%= render 'proponents/contacts/datagrid', contacts: @contacts %>
    </div>
  </div>
<% end %>
